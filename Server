#include<stdio.h>
#include<sys/types.h>
#include<sys/socket.h>
#include<netinet/in.h>

int main(int argc , int *argv[]){
int socketFeedback 
/** 
functionallity: create new socket
return: int (entry to descriptor file) neg means error
arg0: address domain type AF_INET or AF_UNIX
arg1: socket type stream or datagram
arg3: protocol 0 means that the os will chose the appropriate protocol (TCP for stream and udp for dtagram)


**/
socketFeedback = socket(AF_INET,SOCK_STREAM,0);
	if(socketFeedback<0){
		error("Server :can't creat socket\n");
	}
	else{
		printf("socket create succssfully\n");	
	}


}
